{
    "name": "pictastudio/laravel-translatable",
    "description": "A Laravel package for multilingual models",
    "license": "MIT",
    "keywords": [
        "laravel",
        "translation",
        "language",
        "database"
    ],
    "homepage": "https://github.com/pictastudio/laravel-translatable",
    "authors": [
        {
            "name": "Picta Studio",
            "email": "info@pictastudio.com",
            "role": "Developer"
        },
        {
            "name": "Frameck",
            "email": "fra9879@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0 || ^3.0",
        "laravel/legacy-factories": "^1.0.4",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PictaStudio\\Translatable\\": "src/Translatable/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PictaStudio\\Translatable\\TranslatableServiceProvider"
            ]
        }
    },
    "scripts": {
        "fix": "@php vendor/bin/pint",
        "phpstan": "@php vendor/bin/phpstan analyse",
        "test": "@php vendor/bin/phpunit",
        "test-coverage": "@php vendor/bin/phpunit --coverage-html=build"
    }
}
