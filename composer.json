{
    "name": "pictastudio/translatable",
    "description": "make your models translatable",
    "keywords": [
        "Picta Studio",
        "laravel",
        "translatable"
    ],
    "homepage": "https://github.com/pictastudio/translatable",
    "license": "MIT",
    "authors": [
        {
            "name": "Picta Studio",
            "email": "info@pictastudio.com",
            "role": "Developer"
        },
        {
            "name": "Frameck",
            "email": "fra9879@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "orchestra/testbench": "^10.4",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PictaStudio\\Translatable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PictaStudio\\Translatable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
